  "main": "index.js",
  "scripts": {
    "watch": "watchify -t babelify ./src/app1.js -o ./dist/bundle1.js",
    "build": "browserify -t babelify ./src/app1.js | uglifyjs -c warnings=false > ./dist/bundle1.js",
    "watch2": "watchify -t babelify ./src/app2.js -o ./dist/bundle2.js",
    "build2": "browserify -t babelify ./src/app2.js | uglifyjs -c warnings=false > ./dist/bundle2.js"
  },
